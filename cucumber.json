{
    "default": {
        "requireModule": [
            "ts-node/register"
        ],
        "require": [
            "src/test/steps/**/**/*.ts",
            "src/test/hooks/**/*.ts"
        ],
        "paths": [
            "src/test/features/**/**/*.feature"
        ],
        "format": [
            "progress-bar"
        ],
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "parallel": 0
    }
}